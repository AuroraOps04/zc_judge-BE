<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="edu.zc.oj.mapper.TagMapper">
    <select id="getTagsByProblem" resultType="edu.zc.oj.entity.Tag">
        select * from tag where id in(select tag_id from problem_tag where problem_id = #{problemId})
    </select>
</mapper>